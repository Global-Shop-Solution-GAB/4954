'F.Intrinsic.Debug.InvokeDebugger
'
'F.Intrinsic.Debug.Stop

f.Intrinsic.Control.If(v.Args.EmployeeNo,<>,"")
	v.Local.sEmployeeNo.Set(v.Args.EmployeeNo)
f.Intrinsic.Control.Else
	'EMPID
	Function.Global.Security.GetEmpNoFromUser(v.Caller.User,v.Local.sEmployeeNo)
f.Intrinsic.Control.EndIf 



F.Intrinsic.String.Build("select Name,Date_Hire,comments_1,comments_2  from v_employee_mstr where employee = '{0}'",v.Local.sEmployeeNo,V.Local.sSQL)
F.ODBC.Connection!con.OpenLocalRecordsetRO("rst",V.Local.sSQL)
F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,<>,True)	
	gui.frmTestReport.dtpcalibrationdt.value(V.ODBC.con!rst.FieldValtrim!Date_Hire)

	gui.frmTestReport.txtguageSN.Text(V.ODBC.con!rst.FieldValtrim!comments_1)                       
 	gui.frmTestReport.txtmakemodel.Text(V.ODBC.con!rst.FieldValtrim!comments_2)
	gui.frmTestReport.lblEmployee.Caption(v.Local.sEmployeeNo)   
	gui.frmTestReport.lblTesterName.Caption(V.ODBC.con!rst.FieldValtrim!Name)
f.Intrinsic.Control.Else
	f.Intrinsic.UI.Msgbox("Employee information not found, Cannot Save Test Report")        
	gui.frmTestReport.cmdTestReportPrint.Visible(false)
	gui.frmTestReport.cmdTestReportSave.Enabled(false)
	gui.frmTestReport.lblEmployee.Caption("")         
