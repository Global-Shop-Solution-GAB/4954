F.Intrinsic.Math.Mult(V.Local.iWinDX,V.Ambient.TwipsPerPixelX,V.Local.iWinDX)
F.Intrinsic.Math.Mult(V.Local.iWinDY,V.Ambient.TwipsPerPixelY,V.Local.iWinDY)
'Placing screen shot onto borderless form with picture box


f.Intrinsic.String.Build("{0}\Custom\4954\",v.Caller.GlobalDir,V.Local.sFolder)
F.Intrinsic.String.Build("{0}-{1}.png",v.Global.sOrder,v.Global.sLine,V.Local.sFileName)
