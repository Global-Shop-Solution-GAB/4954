f.Intrinsic.File.Exists(V.Local.sFileName,V.Local.bRet)
f.Intrinsic.Control.If(V.Local.bRet)
	Gui.frmMaint.pic1.Picture(V.Local.sFileName)
	'Size screen shot form and picture box to fill form
	Gui.frmMaint..Size(V.Local.iWinDX,V.Local.iWinDY)
	Gui.frmMaint.pic1.Position(0,0)
	Gui.frmMaint.pic1.BorderStyle(0)
	Gui.frmMaint.pic1.Size(V.Local.iWinDX,V.Local.iWinDY)
	gui.frmMaint.pic1.Visible(true)
	v.Global.bScrShotV.Set(True)
	Gui.frmMaint..Show
